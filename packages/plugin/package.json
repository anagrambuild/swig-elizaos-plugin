{
  "name": "@swig-wallet/plugin-elizaos",
  "version": "1.0.1",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --clean --format esm --out-dir dist",
    "dev": "tsup src/index.ts --dts --watch --format esm --out-dir dist",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "eslint --ext .ts src/ --fix",
    "test": "vitest"
  },
  "dependencies": {
    "@elizaos/core": "^1.0.2",
    "@solana/codecs": "^2.1.0",
    "@solana/spl-token": "^0.4.9",
    "@solana/web3.js": "^1.98.2",
    "@swig-wallet/classic": "0.2.0-beta.4",
    "bs58": "^6.0.0",
    "rpc-websockets": "^10.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.22.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.5"
  },
  "peerDependencies": {
    "@elizaos/core": "^1.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://onswig.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/anagrambuild/swig-elizaos-plugin"
  },
  "license": "AGPL-3.0",
  "description": "Swig wallet plugin for ElizaOS",
  "keywords": [
    "eliza",
    "elizaos",
    "plugin",
    "swig",
    "solana",
    "wallet",
    "smart wallet"
  ]
}
